{
  "manifest_version": 3,
  "name": "Image Keyword Overlay",
  "version": "1.0",
  "description": "Show keywords on images using BLIP.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
